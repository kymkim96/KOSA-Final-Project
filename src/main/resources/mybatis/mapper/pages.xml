<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pages">
	<select id="selectByCategory" parameterType="category" resultType="page" fetchSize="1000">
		select *
		from ${category}
	</select>
</mapper>